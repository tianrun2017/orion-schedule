schedule:
  server:
    codec:
      code: "jackson-msg-pack"

    transport:
      code: "netty4"
      netty4Config:
        serverPort: 9006
        idleTime: 10
        closeWait: 2
        connectionTimeOut: 5000
    register:
      code: "nacos"
      registerEnv: "DEVELOP"
      nacosConfig:
        namespace: "431b3916-40d9-4710-aa25-dce3c438cf2b"
        serverList:
          - "10.107.117.30:8848"
    task:
      exeLogOpen: true
      dataSource:
        url: ${JDBC_URL}
        username: ${JDBC_USER}
        password: ${JDBC_PASSWORD}
        pollSize: 10
      processThread: 10
      groupList:
        - "101"
        - "102"
